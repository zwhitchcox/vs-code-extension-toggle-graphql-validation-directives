{
	"name": "vs-code-extension-toggle-graphql-validation-directives",
	"displayName": "toggle-graphql-validation-directives",
	"description": "Easily collapse and uncollapse graphql validation directives with a quick command",
	"version": "0.0.1",
	"publisher": "zwhitchcox",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"main": "./out/extension.js",
	"activationEvents": [
		"onLanguage:graphql"
	],
	"repository": "https://github.com/zwhitchcox/vs-code-extension-toggle-graphql-validation-directives",
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"fs-extra": "^8.0.1",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"@types/fs-extra": "^7.0.0",
		"@types/graphql": "^14.2.0",
		"graphql": "^14.3.0"
	}
}
